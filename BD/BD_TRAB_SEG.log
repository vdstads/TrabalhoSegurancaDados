/*C3*/SET SCHEMA PUBLIC
DROP TABLE DEPARTAMENTO
CREATE TABLE DEPARTAMENTO(\u000a\u0009CODIGO VARCHAR(255) not null primary key,\u000a\u0009NOME VARCHAR(255) not null,\u000a\u0009FK_GERENTE_CPF VARCHAR(14) not null,\u000a\u0009CONSTRAINT CPF_GER FOREIGN KEY (FK_GERENTE_CPF) REFERENCES FUNCIONARIO (CPF)\u000a)
DROP TABLE PROJETO
CREATE TABLE PROJETO(\u000a\u0009COD_PROJETO INT NOT NULL PRIMARY KEY,\u000a\u0009NOME VARCHAR(255) not null,\u000a\u0009DESCRICAO VARCHAR(255) not null,\u000a\u0009DATA_INICIO VARCHAR(12) not null,\u000a\u0009DATA_TERMINO VARCHAR(12) not null,\u000a\u0009FK_DEPARTAMENTO VARCHAR(255),\u000a\u0009CONSTRAINT COD_DEPARTAMENTO FOREIGN KEY (FK_DEPARTAMENTO) REFERENCES DEPARTAMENTO (CODIGO)\u000a)
DROP TABLE ATIVIDADE
CREATE TABLE ATIVIDADE(\u000a\u0009COD_ATIVIDADE INT NOT NULL PRIMARY KEY,\u000a\u0009NOME VARCHAR(255) not null,\u000a\u0009DURACAO_PREV VARCHAR(12) not null,\u000a\u0009HORAS_TRABALHADAS INT,\u000a\u0009PERCENTUAL_CONCLUSAO INT,\u000a\u0009FK_ENCARREGADO_CPF VARCHAR(14) not null,\u000a\u0009FK_PROJETO INT,\u000a\u0009CONSTRAINT CPF FOREIGN KEY (FK_ENCARREGADO_CPF) REFERENCES FUNCIONARIO (CPF),\u000a\u0009CONSTRAINT COD_PROJETO FOREIGN KEY (FK_PROJETO) REFERENCES PROJETO (COD_PROJETO)\u000a)
DISCONNECT
